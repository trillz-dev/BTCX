<div class="profile-summary">
    <div class="profile-summary-images">
        <div>
            <img class="img-fluid" src="/public/images/Combined Shape Copy.png" alt="">
        </div>
        <div class="profile-summary-img">
            <img class="img-fluid profile-img" src="/public/images/avatar/avatar1.jpg" alt="" >
        </div>
    </div>

    <div>
        <h3 class="profile-summary-name text-capitalize"><%= userName %></h3>
    </div>

    <div class="profile-summary-contact-wrapper">
        <div class="profile-summary-contact-header text-uppercase">
            <div>
                <p class="profile-summary-info-title">contact info</p>
            </div>
            <!-- <div>
                <a class="profile-summary-info-title" href=""><span><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/>
                  </svg></span>update</a>
            </div> -->
        </div>
        <div class="profile-summary-contact-body text-capitalize">
            <div class="profile-summary-contact-email">
                <div>
                    <p class="profile-summary-info">email</p>
                </div>
                <div>
                    <p class="profile-summary-info"><%= userEmail %></p>
                </div>
            </div>

            <div class="profile-summary-contact-phone">
                <div>
                    <p class="profile-summary-info">phone</p>
                </div>
                <div>
                    <p class="profile-summary-info"><%= userNumber %></p>
                </div>
            </div>
        </div>
    </div>

    <div class="profile-summary-bank-wrapper">
        <div class="profile-summary-bank-header text-uppercase">
            <div>
                <p class="profile-summary-info-title">bank details</p>
            </div>
            <div>
                <a class="profile-summary-info-title" href=""><span><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/>
                  </svg></span>update</a>
            </div>
        </div>
        <div class="profile-summary-bank-body text-capitalize">
            <div class="profile-summary-bank-email">
                <div>
                    <p class="profile-summary-info">bank</p>
                </div>
                <div>
                    <p class="profile-summary-info">wells fargo</p>
                </div>
            </div>

            <div class="profile-summary-bank-phone">
                <div>
                    <p class="profile-summary-info">account</p>
                </div>
                <div>
                    <p class="profile-summary-info">0123456789</p>
                </div>
            </div>
        </div>
    </div>

    <div class="profile-summary-crypto-wrapper">
        <div class="profile-summary-crypto-header text-uppercase">
            <div>
                <p class="profile-summary-info-title">crypto wallet</p>
            </div>
            <div>
                <a class="profile-summary-info-title" href=""><span><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/>
                  </svg></span>update</a>
            </div>
        </div>
        <div class="profile-summary-crypto-body text-capitalize">
            <div class="profile-summary-crypto-name">
                <div>
                    <p class="profile-summary-info">bitcoin</p>
                </div>
                <div>
                    <p class="profile-summary-info">0123456789</p>
                </div>
            </div>

            <div class="profile-summary-crypto-name">
                <div>
                    <p class="profile-summary-info">etherum</p>
                </div>
                <div>
                    <p class="profile-summary-info"></p>
                </div>
            </div>

            <div class="profile-summary-crypto-name">
                <div>
                    <p class="profile-summary-info">litcoin</p>
                </div>
                <div>
                    <p class="profile-summary-info"></p>
                </div>
            </div>
        </div>
    </div>

</div>